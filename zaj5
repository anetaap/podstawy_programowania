#include <stdio.h>


void podwojenie(int tab[], int n)
{
    for (int i=0; i<n; ++i)
        {
            tab[i]*=2;
        }
    for (int i=0; i<n; ++i)
        {
        printf("%d", tab[i]);
        }
}


int main()
{
    int tab[]={3,4,6,7,8};
    int n= 5;
    
    podwojenie(tab,n);
    
    return 0;
}
